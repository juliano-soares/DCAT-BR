<?xml version="1.0" encoding="utf-8" ?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:skos="http://www.w3.org/2004/02/skos/core#"
         xmlns:dc="http://purl.org/dc/terms/"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#">

  <skos:ConceptScheme rdf:about="https://dcat-br.gov.br/vocabularies/VCR-CE/esquema">
    <dc:title xml:lang="pt-BR">Vocabulário Controlado de Cobertura Espacial (VCR-CE) - DCAT-BR</dc:title>
    <dc:description xml:lang="pt-BR">Vocabulário SKOS para descrever a cobertura espacial de distribuições de dados em catálogos de dados públicos no Brasil, classificando a abrangência territorial em níveis federais, estaduais e municipais.</dc:description>
    <dc:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-12-11</dc:issued>
    <dc:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-12-11</dc:modified>
    <dc:language>pt-BR</dc:language>
    <dc:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
    <dc:publisher rdf:resource="https://dcat-br.gov.br/"/>
    
    <skos:hasTopConcept rdf:resource="https://dcat-br.gov.br/vocabularies/VCR-CE/FEDERAL"/>
    <skos:hasTopConcept rdf:resource="https://dcat-br.gov.br/vocabularies/VCR-CE/ESTADUAL"/>
    <skos:hasTopConcept rdf:resource="https://dcat-br.gov.br/vocabularies/VCR-CE/MUNICIPAL"/>
  </skos:ConceptScheme>

  <skos:Concept rdf:about="https://dcat-br.gov.br/vocabularies/VCR-CE/FEDERAL">
    <skos:inScheme rdf:resource="https://dcat-br.gov.br/vocabularies/VCR-CE/esquema"/>
    <skos:topConceptOf rdf:resource="https://dcat-br.gov.br/vocabularies/VCR-CE/esquema"/>
    <skos:prefLabel xml:lang="pt-BR">Federal</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Federal</skos:prefLabel>
    <skos:notation>FEDERAL</skos:notation>
    <skos:definition xml:lang="pt-BR">Cobertura espacial de abrangência federal, referente a dados que se aplicam a todo o território nacional brasileiro.</skos:definition>
    <skos:definition xml:lang="en">Federal spatial coverage, referring to data that applies to the entire Brazilian national territory.</skos:definition>
    <dc:identifier>FEDERAL</dc:identifier>
  </skos:Concept>

  <skos:Concept rdf:about="https://dcat-br.gov.br/vocabularies/VCR-CE/ESTADUAL">
    <skos:inScheme rdf:resource="https://dcat-br.gov.br/vocabularies/VCR-CE/esquema"/>
    <skos:topConceptOf rdf:resource="https://dcat-br.gov.br/vocabularies/VCR-CE/esquema"/>
    <skos:prefLabel xml:lang="pt-BR">Estadual/Distrital</skos:prefLabel>
    <skos:prefLabel xml:lang="en">State</skos:prefLabel>
    <skos:notation>ESTADUAL</skos:notation>
    <skos:definition xml:lang="pt-BR">Cobertura espacial de abrangência estadual ou distrital, referente a dados que se aplicam a um ou mais estados brasileiros ou ao Distrito Federal.</skos:definition>
    <skos:definition xml:lang="en">State-level or district-level spatial coverage, referring to data that applies to one or more Brazilian states or to the Federal District.</skos:definition>
    <dc:identifier>ESTADUAL</dc:identifier>
  </skos:Concept>

  <skos:Concept rdf:about="https://dcat-br.gov.br/vocabularies/VCR-CE/MUNICIPAL">
    <skos:inScheme rdf:resource="https://dcat-br.gov.br/vocabularies/VCR-CE/esquema"/>
    <skos:topConceptOf rdf:resource="https://dcat-br.gov.br/vocabularies/VCR-CE/esquema"/>
    <skos:prefLabel xml:lang="pt-BR">Municipal</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Municipal</skos:prefLabel>
    <skos:notation>MUNICIPAL</skos:notation>
    <skos:definition xml:lang="pt-BR">Cobertura espacial de abrangência municipal, referente a dados que se aplicam a um ou mais municípios brasileiros.</skos:definition>
    <skos:definition xml:lang="en">Municipal spatial coverage, referring to data that applies to one or more Brazilian municipalities.</skos:definition>
    <dc:identifier>MUNICIPAL</dc:identifier>
  </skos:Concept>

</rdf:RDF>
