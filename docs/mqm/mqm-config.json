    "metrics": {
      "encontrabilidade": [
        {
          "id": "dcat_keyword_present",
          "property": "dcat:keyword",
          "dimension": "encontrabilidade",
          "validations": [
            {
              "id": "dcat_keyword_is_present",
              "type": "property_exists",
              "label": "Utilização de palavra-chave",
              "description": "As palavras-chave apoiam diretamente a pesquisa e aumentam a capacidade de descoberta. Verifica se dcat:keyword está definido.",
              "weight": 20,
              "check": {
                "minCount": 1,
                "notEmpty": true
              }
            }
          ]
        },
        {
          "id": "dcat_theme_present",
          "property": "dcat:theme",
          "dimension": "encontrabilidade",
          "validations": [
            {
              "id": "dcat_theme_is_present",
              "type": "property_exists",
              "label": "Tema",
              "description": "As categorias ajudam os usuários a explorar tematicamente os conjuntos de dados. Verifica se dcat:theme foi atribuído.",
              "weight": 30,
              "check": {
                "minCount": 1
              }
            },
            {
              "id": "dcat_theme_vocabulary",
              "type": "vocabulary_check",
              "label": "Vocabulário de tema",
              "description": "Verifica se algum dos Vocabulários Controlados de Referência (VCR) foi usado para tema.",
              "weight": 10,
              "vocabularies": ["themes"]
            }
          ]
        },
        {
          "id": "dct_spatial_present",
          "property": "dct:spatial",
          "dimension": "encontrabilidade",
          "validations": [
            {
              "id": "dct_spatial_is_present",
              "type": "property_exists",
              "label": "Pesquisa espacial",
              "description": "A informação espacial habilita os usuários a encontrar o conjunto de dados com uma pesquisa espacial. Verifica se dct:spatial foi definido.",
              "weight": 20,
              "check": {
                "minCount": 1
              }
            }
          ]
        },
        {
          "id": "dct_temporal_present",
          "property": "dct:temporal",
          "dimension": "encontrabilidade",
          "validations": [
            {
              "id": "dct_temporal_is_present",
              "type": "temporal_check",
              "label": "Pesquisa temporal",
              "description": "Verifica se metadados temporais (ex: dct:temporal, dcat:startDate, dcat:endDate) foram definidos, permitindo a filtragem de dados por período.",
              "weight": 20,
              "check": {
                "properties": ["dct:temporal", "dcat:startDate", "dcat:endDate"],
                "minCount": 1
              }
            }
          ]
        }
      ],
      "acessibilidade": [
        {
          "id": "dcat_accessURL_valid",
          "property": "dcat:accessURL",
          "dimension": "acessibilidade",
          "validations": [
            {
              "id": "dcat_accessURL_is_valid",
              "type": "url_check",
              "label": "Acessibilidade da URL de acesso",
              "description": "A URL de acesso (dcat:accessURL) pode levar diretamente aos dados ou a uma página com mais informações. A URL é verificada via requisição HTTP, esperando um código de estado entre 200 e 300.",
              "weight": 50,
              "check": {
                "minCount": 1,
                "statusCodes": [200, 201, 202, 203, 204, 205, 206, 207, 208, 226, 300, 301, 302, 303, 304, 305, 306, 307, 308]
              }
            }
          ]
        },
        {
          "id": "dcat_downloadURL_present",
          "property": "dcat:downloadURL",
          "dimension": "acessibilidade",
          "validations": [
            {
              "id": "dcat_downloadURL_is_present",
              "type": "property_exists",
              "label": "URL de transferência",
              "description": "A URL de transferência (dcat:downloadURL) deve ser um link direto para o download dos dados. Verifica se a propriedade foi definida.",
              "weight": 50,
              "check": {
                "minCount": 1
              }
            }
          ]
        }
      ],
