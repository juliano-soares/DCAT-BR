@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix dct:  <http://purl.org/dc/terms/> .

#
# DCAT-BR 1.0.0 — mdr_imports.ttl
#
# Este arquivo fornece as importações das listas de códigos recomendadas
# pelo perfil de aplicação DCAT-BR (VCR – Vocabulários Controlados de Referência),
# para uso em validações SHACL (MDR – Managed Data References).


<http://purl.org/dcat-br/vocabularies/mdr_imports>
    a owl:Ontology ;
    rdfs:label "DCAT-BR — MDR imports (VCR)"@en ;
    rdfs:label "DCAT-BR — Importações MDR (VCR)"@pt-BR ;
    rdfs:comment "Importações (MDR) de listas de códigos do DCAT-BR / VCR."@pt-BR ;
    rdfs:comment "DCAT-BR VCR code list imports (MDR)."@en ;
    dct:issued "2025-12-05"^^xsd:date ;
    owl:versionInfo "1.0.0" ;

    #####################################################################
    # Vocabulários VCR — Perfil Brasileiro (DCAT-BR)
    #####################################################################

    # Frequência de atualização (VCR-FR)
    # Baseado/compatível com CLD Frequency (Dublin Core Collection Description)
    owl:imports <http://purl.org/dcat-br/vocabularies/VCR-FR.ttl> ;

    # Licença de uso (VCR-LU)
    # Inclui CC (Creative Commons), ODbL e conceitos nacionais (ex.: uso restrito)
    owl:imports <http://purl.org/dcat-br/vocabularies/VCR-LU.ttl> ;

    # Idioma (VCR-LN)
    # Mapeado a ISO 639-1 / ISO 639-2 (Library of Congress)
    owl:imports <http://purl.org/dcat-br/vocabularies/VCR-LN.ttl> ;

    # Cobertura espacial / abrangência administrativa (VCR-CE)
    # Federal / Estadual / Municipal
    owl:imports <http://purl.org/dcat-br/vocabularies/VCR-CE.ttl> ;

    # Tipo de recurso (VCR-TR)
    # Ex.: Dados, Dicionário de Dados, Documentação, API, Outro
    owl:imports <http://purl.org/dcat-br/vocabularies/VCR-TR.ttl> ;

    #####################################################################
    # Vocabulários externos de referência (bases semânticas)
    #####################################################################

    # ISO 639-1 — Library of Congress
    # Base canônica para idiomas
    owl:imports <https://id.loc.gov/vocabulary/iso639-1.rdf> ;

    # SPDX Ontology
    # Usada para checksum (spdx:Checksum) e apoio a licenças
    owl:imports <https://spdx.org/rdf/terms/spdx-ontology.owl.xml> ;

    .
