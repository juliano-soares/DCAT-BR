    ], [
        sh:path foaf:page ;
        sh:nodeKind sh:BlankNodeOrIRI ;
        sh:severity sh:Violation ;
        sh:message "foaf:page deve ser um recurso (URI/IRI ou nó anônimo) apontando para uma página web relacionada."@pt-BR
    ] .

