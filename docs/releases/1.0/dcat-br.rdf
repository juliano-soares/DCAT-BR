<?xml version="1.0"?>
<!DOCTYPE rdf:RDF [
    <!ENTITY dct "http://purl.org/dc/terms/" >
    <!ENTITY foaf "http://xmlns.com/foaf/0.1/" >
    <!ENTITY dcat "http://www.w3.org/ns/dcat#" >
    <!ENTITY adms "http://www.w3.org/ns/adms#" >
    <!ENTITY vann "http://purl.org/vocab/vann/" >
    <!ENTITY xhv "http://www.w3.org/1999/xhtml/vocab#">
    <!ENTITY cc "http://creativecommons.org/ns#" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY vcard "http://www.w3.org/2006/vcard/ns#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY wdrs "http://www.w3.org/2007/05/powder-s#" >
    <!ENTITY voaf "http://purl.org/vocommons/voaf#" >
    <!ENTITY skos "http://www.w3.org/2004/02/skos/core#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY locn "http://www.w3.org/ns/locn#">
    <!ENTITY prov "http://www.w3.org/ns/prov#">
    <!ENTITY time "http://www.w3.org/2006/time#">
    <!ENTITY odrl "http://www.w3.org/ns/odrl/2/">
    <!ENTITY schema "http://schema.org/">
    <!ENTITY spdx "http://spdx.org/rdf/terms#">
    <!ENTITY dcatbr "http://purl.org/dcat-br/">
    <!ENTITY dcterms "http://purl.org/dc/terms/">

    <!ENTITY documentation "https://dcat-br.gov.br/releases/1.0/dcat-br.html">
    <!ENTITY documentationTitle "DCAT-BR 1.0.0">
]>
<rdf:RDF
    xmlns:rdf="&rdf;"
    xmlns:rdfs="&rdfs;"
    xmlns:xsd="&xsd;"
    xmlns:owl="&owl;"
    xmlns:dct="&dct;"
    xmlns:dcat="&dcat;"
    xmlns:dcterms="&dcterms;"
    xmlns:adms="&adms;"
    xmlns:foaf="&foaf;"
    xmlns:skos="&skos;"
    xmlns:vann="&vann;"
    xmlns:voaf="&voaf;"
    xmlns:vcard="&vcard;"
    xmlns:wdrs="&wdrs;"
    xmlns:locn="&locn;"
    xmlns:prov="&prov;"
    xmlns:time="&time;"
    xmlns:odrl="&odrl;"
    xmlns:schema="&schema;"
    xmlns:spdx="&spdx;"
    xmlns:dcatbr="&dcatbr;"
    xmlns:xhv="&xhv;"
    xmlns:cc="&cc;">

    <!-- ========================================================= -->
    <!-- Ontologia do Perfil de Aplicação DCAT-BR                  -->
    <!-- ========================================================= -->

    <owl:Ontology>
        <rdfs:label xml:lang="pt-BR">Vocabulário Padrão DCAT-BR</rdfs:label>
        <rdfs:label xml:lang="en">DCAT-BR Application Profile</rdfs:label>

        <dct:abstract xml:lang="pt-BR">
            O DCAT-BR é um perfil de aplicação do vocabulário Data Catalog (DCAT) para
            descrever conjuntos de dados, recursos, distribuições, serviços de dados e
            metadados relacionados no contexto do setor público brasileiro.
        </dct:abstract>
        <dct:abstract xml:lang="en">
            DCAT-BR is an application profile of the Data Catalog Vocabulary (DCAT) for
            describing datasets, resources, distributions, data services and related
            metadata for Brazilian public sector data.
        </dct:abstract>

        <dct:modified rdf:datatype="&xsd;date">2025-12-12</dct:modified>
        <owl:versionInfo>1.0.0</owl:versionInfo>

        <cc:attributionName xml:lang="pt-BR">Governo Federal do Brasil</cc:attributionName>
        <cc:attributionURL rdf:resource="https://www.gov.br/"/>

        <voaf:specializes rdf:resource="http://www.w3.org/ns/dcat#"/>

        <dct:publisher rdf:parseType="Resource">
            <foaf:name>Ministério da Gestão e da Inovação em Serviços Públicos</foaf:name>
            <foaf:homepage rdf:resource="https://www.gov.br/gestao/pt-br"/>
        </dct:publisher>

        <dct:license>
            <rdf:Description rdf:about="https://creativecommons.org/licenses/by/4.0/">
                <dct:title>Attribution 4.0 International (CC BY 4.0)</dct:title>
                <cc:attributionName>Governo Federal do Brasil</cc:attributionName>
                <cc:attributionURL rdf:resource="https://www.gov.br/"/>
            </rdf:Description>
        </dct:license>

        <dct:conformsTo>
            <rdf:Description rdf:about="&documentation;">
                <dct:title>&documentationTitle;</dct:title>
            </rdf:Description>
        </dct:conformsTo>

        <dct:creator rdf:parseType="Resource">
            <foaf:name>Juliano Leonardo Soares</foaf:name>
            <foaf:homepage rdf:resource="https://www.linkedin.com/in/juliano-leonardo-soares/"/>
            <schema:affiliation rdf:parseType="Resource">
                <foaf:name>CPQD</foaf:name>
                <foaf:homepage rdf:resource="https://cpqd.com.br"/>
            </schema:affiliation>
        </dct:creator>
    </owl:Ontology>

    <!-- ========================================================= -->
    <!-- CLASSES DCAT-BR                                           -->
    <!-- ========================================================= -->
        
    <!-- Conjunto de Dados (dcat:Dataset) -->
    <rdf:Description rdf:about="&dcat;Dataset">
        <dct:identifier>dcat:Dataset</dct:identifier>
        <rdfs:label xml:lang="pt-BR">Conjunto de Dados</rdfs:label>
        <rdfs:label xml:lang="en">Dataset</rdfs:label>
        <rdfs:comment xml:lang="pt-BR">
            Uma coleção de dados, publicada ou com curadoria de um único agente, e disponível para acesso ou download em uma ou mais representações.
        </rdfs:comment>
    </rdf:Description>

    <!-- Recurso / Recurso Catalogado (dcat:Resource) -->
    <rdf:Description rdf:about="&dcat;Resource">
        <dct:identifier>dcat:Resource</dct:identifier>
        <rdfs:label xml:lang="pt-BR">Recurso (Conjunto de Dados)</rdfs:label>
        <rdfs:label xml:lang="en">Resource (Dataset)</rdfs:label>
        <rdfs:comment xml:lang="pt-BR">
            Recurso publicado ou curado por um único agente.
        </rdfs:comment>
    </rdf:Description>

    <!-- Distribuição (dcat:Distribution) -->
    <rdf:Description rdf:about="&dcat;Distribution">
        <dct:identifier>dcat:Distribution</dct:identifier>
        <rdfs:label xml:lang="pt-BR">Distribuição</rdfs:label>
        <rdfs:label xml:lang="en">Distribution</rdfs:label>
        <rdfs:comment xml:lang="pt-BR">
            Uma representação específica de um conjunto de dados. Um conjunto de dados pode estar disponível em várias serializações que podem diferir de várias maneiras, incluindo linguagem natural, tipo ou formato de mídia, organização esquemática, resolução temporal e espacial, nível de detalhe ou perfis (que podem especificar qualquer um ou todos os itens acima).
        </rdfs:comment>
    </rdf:Description>

    <!-- Período de Tempo (dct:PeriodOfTime) -->
    <rdf:Description rdf:about="&dct;PeriodOfTime">
        <dct:identifier>dcterms:PeriodOfTime</dct:identifier>
        <rdfs:label xml:lang="pt-BR">Período de Tempo</rdfs:label>
        <rdfs:label xml:lang="en">Period of Time</rdfs:label>
        <rdfs:comment xml:lang="pt-BR">
            Um intervalo de tempo que é nomeado ou definido por seu início e fim.
        </rdfs:comment>
    </rdf:Description>

    <!-- Organização / Pessoa (foaf:Organization / foaf:Person) -->
    <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Agent">
    	<dct:identifier>foaf:Organization / foaf:Person</dct:identifier>
    	<rdfs:label xml:lang="en">Organization / Person</rdfs:label>
    	<rdfs:isDefinedBy rdf:resource="http://xmlns.com/foaf/spec/#term_Agent" />
    	<rdfs:comment xml:lang="pt-BR">
            [FOAF] Fornece diversas propriedades para descrever pessoas, organizações ou entidades.
        </rdfs:comment>
    </rdf:Description>

    <!-- Local (dct:Location) -->
    <rdf:Description rdf:about="&dct;Location">
        <dct:identifier>dcterms:Location</dct:identifier>
        <rdfs:label xml:lang="pt-BR">Local</rdfs:label>
        <rdfs:label xml:lang="en">Location</rdfs:label>
        <rdfs:comment xml:lang="pt-BR">Uma região espacial ou lugar nomeado.</rdfs:comment>
    </rdf:Description>

    <!-- Serviço de Dados (dcat:DataService) -->
    <rdf:Description rdf:about="&dcat;DataService">
        <dct:identifier>dcat:DataService</dct:identifier>
        <rdfs:label xml:lang="pt-BR">Serviço de Dados</rdfs:label>
        <rdfs:label xml:lang="en">Data Service</rdfs:label>
        <rdfs:comment xml:lang="pt-BR">
            Uma coleção de operações que fornece acesso a um ou mais conjuntos de dados ou funções de processamento de dados.
        </rdfs:comment>
    </rdf:Description>

    <!-- Verificação de Integridade (spdx:Checksum) -->
    <rdf:Description rdf:about="&spdx;Checksum">
        <dct:identifier>spdx:Checksum</dct:identifier>
        <rdfs:label xml:lang="pt-BR">Verificação de Integridade (Checksum)</rdfs:label>
        <rdfs:label xml:lang="en">Checksum</rdfs:label>
        <rdfs:comment xml:lang="pt-BR">
            Um Checksum é um valor que permite verificar a integridade do conteúdo de um arquivo. Mesmo pequenas alterações no conteúdo do arquivo alterarão seu Checksum. Esta classe permite que os resultados de uma variedade de algoritmos de Checksum e resumo de mensagens criptográficas sejam representados.
        </rdfs:comment>
    </rdf:Description>

    <!-- ####################################### -->
    <!-- ##===================================## -->
    <!-- ##==          PROPERTIES           ==## -->
    <!-- ##===================================## --> 
    <!-- ####################################### -->
     
    <!-- ======================================= -->
    <!-- ==  Conjunto de Dados - Properties   == -->
    <!-- ======================================= --> 
    
    <!-- Periodicidade de Atualização -->
    <rdf:Description rdf:about="&dcterms;accrualPeriodicity">
        <dct:identifier>dcterms:accrualPeriodicity</dct:identifier>

        <rdfs:label xml:lang="pt-BR">Periodicidade de Atualização</rdfs:label>
        <rdfs:label xml:lang="en">Accrual Periodicity</rdfs:label>

        <vann:usageNote xml:lang="pt-BR">
            Deve indicar qual será a periodicidade com que o conjunto de dados será atualizado
            (ex.: diária, mensal, trimestral, semestral, anual).
            (Selo de Qualidade: Bronze)
        </vann:usageNote>

        <rdfs:comment xml:lang="pt-BR">
            Deve indicar qual será a periodicidade com que o conjunto de dados será atualizado.
        </rdfs:comment>

        <!-- Domínio e alcance (Variação) -->
        <rdfs:domain rdf:resource="&dcat;Dataset"/>
        <rdfs:range rdf:resource="&dcterms;Frequency"/>
    </rdf:Description>

    <!-- Área Geográfica (Cobertura Espacial) -->
    <rdf:Description rdf:about="&dcterms;spatial">
        <dct:identifier>dcterms:spatial</dct:identifier>

        <rdfs:label xml:lang="pt-BR">Área Geográfica</rdfs:label>
        <rdfs:label xml:lang="en">Spatial / Geographical Coverage</rdfs:label>

        <vann:usageNote xml:lang="pt-BR">
            Deve indicar a área geográfica coberta pelo conjunto de dados,
            representando a extensão territorial à qual os dados se referem.
            (Selo de Qualidade: Ouro)
        </vann:usageNote>

        <rdfs:comment xml:lang="pt-BR">
            A área geográfica coberta pelo conjunto de dados.
        </rdfs:comment>

        <!-- Domínio e alcance (Variação) -->
        <rdfs:domain rdf:resource="&dcat;Dataset"/>
        <rdfs:range rdf:resource="&dcterms;Location"/>
    </rdf:Description>
    
    <!-- Período Temporal -->
    <rdf:Description rdf:about="&dcterms;temporal">
        <dct:identifier>dcterms:temporal</dct:identifier>
        
        <rdfs:label xml:lang="pt-BR">Período Temporal</rdfs:label>
        <rdfs:label xml:lang="en">Temporal Coverage</rdfs:label>
        
        <vann:usageNote xml:lang="pt-BR">
            Utilizar para indicar o intervalo temporal coberto pelos dados,
            referenciando um recurso do tipo dcterms:PeriodOfTime.
        </vann:usageNote>
       
        <rdfs:comment xml:lang="pt-BR">
            O período temporal que o conjunto de dados cobre.
        </rdfs:comment>

        <rdfs:domain rdf:resource="&dcat;Dataset"/>
        <rdfs:range rdf:resource="&dcterms;PeriodOfTime"/>
    </rdf:Description>

    <!-- Fornecedor Negocial do Conjunto de Dados -->
    <rdf:Description rdf:about="&prov;wasGeneratedBy">
        <dct:identifier>prov:wasGeneratedBy</dct:identifier>
        
        <rdfs:label xml:lang="pt-BR">Fornecedor Negocial do Conjunto de Dados</rdfs:label>
        <rdfs:label xml:lang="en">Was Generated By</rdfs:label>
        
        <vann:usageNote xml:lang="pt-BR">
            Referencie uma instância de prov:Activity descrevendo o processo/atividade que gerou o conjunto de dados
            (ex.: "Cálculo anual do Conceito Enade", "Extração mensal do CNPJ", etc.).
        </vann:usageNote>
        
        <rdfs:comment xml:lang="pt-BR">
            Uma atividade que gerou ou fornece o contexto de negócios para a criação do conjunto de dados.
        </rdfs:comment>
        
        <rdfs:domain rdf:resource="&dcat;Dataset"/>
        <rdfs:range rdf:resource="&prov;Activity"/>
    </rdf:Description>

    <!-- Em Série -->
    <rdf:Description rdf:about="&dcat;inSeries">
        <dct:identifier>dcat:inSeries</dct:identifier>
        
        <rdfs:label xml:lang="pt-BR">Em Série</rdfs:label>
        <rdfs:label xml:lang="en">In Series</rdfs:label>
        
        <vann:usageNote xml:lang="pt-BR">
            Deve indicar se o conjunto de dados faz parte de uma série de conjuntos de dados.
        </vann:usageNote>

        <rdfs:comment xml:lang="pt-BR">
            Uma série de conjuntos de dados da qual o conjunto de dados faz parte.
        </rdfs:comment>
        
        <rdfs:domain rdf:resource="&dcat;Dataset"/>
        <rdfs:range rdf:resource="&prov;Activity"/>
    </rdf:Description>

    <!-- ======================================= -->
    <!-- ==  Recursos - Properties            == -->
    <!-- ======================================= --> 

    <!-- Observância Legal -->
    <rdf:Description rdf:about="&dcterms;accessRights">
        <dct:identifier>dcterms:accessRights</dct:identifier>

        <rdfs:label xml:lang="pt-BR">Observância Legal</rdfs:label>
        <rdfs:label xml:lang="en">Access Rights</rdfs:label>

        <vann:usageNote xml:lang="pt-BR">
            Deve indicar se o conjunto de dados é público, restrito ou privado.
        </vann:usageNote>

        <rdfs:comment xml:lang="pt-BR">
            Deve indicar se o conjunto de dados é público, restrito ou privado.
        </rdfs:comment>

        <!-- Domínio e alcance (Variação) -->
        <rdfs:domain rdf:resource="&dcat;Resource"/>
        <rdfs:range rdf:resource="&dcterms;RightsStatement"/>
    </rdf:Description>

    <!-- E-mail da Área Técnica Responsável -->
    <rdf:Description rdf:about="&dcat;contactPoint">
        <dct:identifier>dcat:contactPoint</dct:identifier>

        <rdfs:label xml:lang="pt-BR">E-mail da Área Técnica Responsável</rdfs:label>
        <rdfs:label xml:lang="en">Contact Point</rdfs:label>

        <vann:usageNote xml:lang="pt-BR">
            Deve indicar o e-mail de contato da área técnica responsável pelo conjunto de dados.
        </vann:usageNote>

        <rdfs:comment xml:lang="pt-BR">
            Deve indicar o e-mail de contato da área técnica responsável pelo conjunto de dados.
        </rdfs:comment>

        <!-- Domínio e alcance (Variação) -->
        <rdfs:domain rdf:resource="&dcat;Resource"/>
        <rdfs:range rdf:resource="&vcard;Kind"/>
    </rdf:Description>

    <!-- Organização -->
    <rdf:Description rdf:about="&dcterms;creator">
        <dct:identifier>dcterms:creator</dct:identifier>

        <rdfs:label xml:lang="pt-BR">Organização</rdfs:label>
        <rdfs:label xml:lang="en">Creator</rdfs:label>

        <vann:usageNote xml:lang="pt-BR">
            Deve indicar qual área técnica do órgão/entidade é responsável pelo conjunto de dados.
        </vann:usageNote>

        <rdfs:comment xml:lang="pt-BR">
            Deve indicar qual área técnica do órgão/entidade é responsável pelo conjunto de dados.
        </rdfs:comment>

        <!-- Domínio e alcance (Variação) -->
        <rdfs:domain rdf:resource="&dcat;Resource"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    </rdf:Description>

    <!-- Descrição -->
    <rdf:Description rdf:about="&dcterms;description">
        <dct:identifier>dcterms:description</dct:identifier>

        <rdfs:label xml:lang="pt-BR">Descrição</rdfs:label>
        <rdfs:label xml:lang="en">Description</rdfs:label>

        <vann:usageNote xml:lang="pt-BR">
            Descrição sobre o conjunto de dados, incluindo metadados que indiquem quais as Políticas Públicas
            ou serviços públicos impactados e metadados que descreva as principais informações do conjunto de dados.
        </vann:usageNote>

        <rdfs:comment xml:lang="pt-BR">
            Descrição sobre o conjunto de dados, incluindo metadados que indiquem quais as Políticas Públicas
            ou serviços públicos impactados e metadados que descreva as principais informações do conjunto de dados.
        </rdfs:comment>

        <!-- Domínio e alcance (Variação) -->
        <rdfs:domain rdf:resource="&dcat;Resource"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </rdf:Description>

    <!-- Palavras-Chave -->
    <rdf:Description rdf:about="&dcat;keyword">
        <dct:identifier>dcat:keyword</dct:identifier>

        <rdfs:label xml:lang="pt-BR">Palavras-Chave</rdfs:label>
        <rdfs:label xml:lang="en">Keyword</rdfs:label>

        <vann:usageNote xml:lang="pt-BR">
            Deve indicar palavras ou tags que resumem os principais aspectos do conjunto de dados.
        </vann:usageNote>

        <rdfs:comment xml:lang="pt-BR">
            Deve indicar palavras ou tags que resumem os principais aspectos do conjunto de dados.
        </rdfs:comment>

        <!-- Domínio e alcance (Variação) -->
        <rdfs:domain rdf:resource="&dcat;Resource"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </rdf:Description>

    <!-- Licença de Uso -->
    <rdf:Description rdf:about="&dcterms;license">
        <dct:identifier>dcterms:license</dct:identifier>

        <rdfs:label xml:lang="pt-BR">Licença de Uso</rdfs:label>
        <rdfs:label xml:lang="en">License</rdfs:label>

        <vann:usageNote xml:lang="pt-BR">
            Deve selecionar sob qual espécie de licença o conjunto de dados será publicado
            de acordo com as definições apresentadas.
        </vann:usageNote>

        <rdfs:comment xml:lang="pt-BR">
            Deve indicar a licença de uso do conjunto de dados.
        </rdfs:comment>

        <!-- Domínio e alcance (Variação) -->
        <rdfs:domain rdf:resource="&dcat;Resource"/>
        <rdfs:range rdf:resource="&dcterms;LicenseDocument"/>
    </rdf:Description>

    <!-- Temas -->
    <rdf:Description rdf:about="&dcat;theme">
        <dct:identifier>dcat:theme</dct:identifier>

        <rdfs:label xml:lang="pt-BR">Tema</rdfs:label>
        <rdfs:label xml:lang="en">Theme / Category</rdfs:label>

        <vann:usageNote xml:lang="pt-BR">
            Deve selecionar a área temática à qual o conjunto de dados esteja vinculado.
            Permite selecionar mais de uma opção.
        </vann:usageNote>

        <rdfs:comment xml:lang="pt-BR">
            Deve indicar a área temática à qual o conjunto de dados está vinculado.
        </rdfs:comment>

        <!-- Domínio e alcance (Variação) -->
        <rdfs:domain rdf:resource="&dcat;Resource"/>
        <rdfs:range rdf:resource="&skos;Concept"/>
    </rdf:Description>

    <!-- Título -->
    <rdf:Description rdf:about="&dcterms;title">
        <dct:identifier>dcterms:title</dct:identifier>

        <rdfs:label xml:lang="pt-BR">Título</rdfs:label>
        <rdfs:label xml:lang="en">Title</rdfs:label>

        <vann:usageNote xml:lang="pt-BR">
            Deve conter um nome dado ao Conjunto de Dados.
            O título deve ser escrito por extenso utilizando sigla após o final do título, caso exista.
        </vann:usageNote>

        <rdfs:comment xml:lang="pt-BR">
            Deve conter um nome dado ao Conjunto de Dados.
            Este é um campo obrigatório para a descrição de um conjunto de dados ou recurso catalogado.
        </rdfs:comment>

        <rdfs:domain rdf:resource="&dcat;Resource"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </rdf:Description>

    <!-- Tipo -->
    <rdf:Description rdf:about="&dcterms;type">
        <dct:identifier>dcterms:type</dct:identifier>

        <rdfs:label xml:lang="pt-BR">Tipo do Recurso</rdfs:label>
        <rdfs:label xml:lang="en">Type</rdfs:label>

        <vann:usageNote xml:lang="pt-BR">
            Deve selecionar se o recurso se refere a Distribuição (arquivo),
            Serviço (API) ou outros.
        </vann:usageNote>

        <rdfs:comment xml:lang="pt-BR">
            Deve indicar o tipo do recurso, como Distribuição (arquivo),
            Serviço (API) ou outros.
        </rdfs:comment>

        <!-- Domínio e alcance (Variação) -->
        <rdfs:domain rdf:resource="&dcat;Resource"/>
        <rdfs:range rdf:resource="&rdfs;Class"/>
    </rdf:Description>

    <!-- Versão -->
    <rdf:Description rdf:about="&dcat;version">
        <dct:identifier>dcat:version</dct:identifier>

        <rdfs:label xml:lang="pt-BR">Versão</rdfs:label>
        <rdfs:label xml:lang="en">Version</rdfs:label>

        <vann:usageNote xml:lang="pt-BR">
            Deve indicar qual é a versão do conjunto de dados que está sendo cadastrada.
            (Selo de Qualidade: Bronze)
        </vann:usageNote>

        <rdfs:comment xml:lang="pt-BR">
            Deve indicar a versão do conjunto de dados.
        </rdfs:comment>

        <!-- Domínio e alcance (Variação) -->
        <rdfs:domain rdf:resource="&dcat;Resource"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </rdf:Description>

    <!-- Área Técnica Responsável -->
    <rdf:Description rdf:about="&dcterms;publisher">
        <dct:identifier>dcterms:publisher</dct:identifier>

        <rdfs:label xml:lang="pt-BR">Área Técnica Responsável</rdfs:label>
        <rdfs:label xml:lang="en">Publisher</rdfs:label>

        <vann:usageNote xml:lang="pt-BR">
            Deve indicar qual área técnica do órgão/entidade é responsável pelo conjunto de dados.
        </vann:usageNote>

        <rdfs:comment xml:lang="pt-BR">
            Deve indicar qual área técnica do órgão/entidade é responsável pela disponibilização
            do conjunto de dados.
        </rdfs:comment>

        <!-- Domínio e alcance (Variação) -->
        <rdfs:domain rdf:resource="&dcat;Resource"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    </rdf:Description>

    <!-- Data de Publicação -->
    <rdf:Description rdf:about="&dcterms;issued">
        <dct:identifier>dcterms:issued</dct:identifier>

