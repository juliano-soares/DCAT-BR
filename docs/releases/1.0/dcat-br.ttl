  rdfs:comment """
            Associa uma localização (área espacial) a uma geometria correspondente.
        """@pt-BR ;
  rdfs:domain dc:Location ;
  rdfs:range rdfs:Resource .

dcat:bbox
  dc:identifier "dcat:bbox" ;
  rdfs:label "Caixa Delimitadora"@pt-BR, "Bounding Box"@en ;
  ns0:usageNote """
            Deve indicar a caixa delimitadora geográfica de uma área espacial.
