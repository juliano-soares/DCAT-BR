        """@pt-BR ;
  rdfs:comment """
            Referência a um padrão ou especificação técnica seguida pela distribuição.
        """@pt-BR ;
  rdfs:domain dcat:Distribution ;
  rdfs:range dc:Standard .

dcat:mediaType
  dc:identifier "dcat:mediaType" ;
