        """@pt-BR ;
  rdfs:comment """
            Centro geográfico (centróide) associado a uma localização.
        """@pt-BR ;
  rdfs:domain dc:Location ;
  rdfs:range rdfs:Literal .

dcat:endpointURL
  dc:identifier "dcat:endpointURL" ;
  rdfs:label "URL do Endpoint"@pt-BR, "Endpoint URL"@en ;
