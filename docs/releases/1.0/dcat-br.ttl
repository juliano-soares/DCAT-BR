        """@pt-BR ;
  rdfs:comment """
            Checksum do arquivo para verificação de integridade.
        """@pt-BR ;
  rdfs:domain dcat:Distribution ;
  rdfs:range <http://spdx.org/rdf/terms#Checksum> .

dcat:startDate
  dc:identifier "dcat:startDate" ;
