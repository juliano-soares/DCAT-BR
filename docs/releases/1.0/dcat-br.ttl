        """@pt-BR ;
  rdfs:comment """
            Caixa delimitadora geográfica (bounding box) de uma localização.
        """@pt-BR ;
  rdfs:domain dc:Location ;
  rdfs:range rdfs:Literal .

dcat:centroid
  dc:identifier "dcat:centroid" ;
  rdfs:label "Centro Geográfico"@pt-BR, "Centroid"@en ;
  ns0:usageNote """
            Deve indicar o centro geográfico (centróide) de uma coisa espacial.
