  rdfs:range rdfs:Literal .

dcat:endDate
  dc:identifier "dcat:endDate" ;
  rdfs:label "Final do Período"@pt-BR, "End Date"@en ;
  ns0:usageNote """
            Deve indicar o final do período.
            (Selo de Qualidade: Ouro)
        """@pt-BR ;
  rdfs:comment """
            Data de fim do período temporal.
        """@pt-BR ;
  rdfs:domain dc:PeriodOfTime ;
  rdfs:range rdfs:Literal .

