        """@pt-BR ;
  rdfs:domain dcat:Resource ;
  rdfs:range dcat:Resource .

#===============================================#
#==  Propriedades para a classe Distribuição ==#
#===============================================#

dcat:accessURL
  dc:identifier "dcat:accessURL" ;
  rdfs:label "URL de Acesso"@pt-BR, "Access URL"@en ;
  ns0:usageNote """
            Deve indicar o endereço da web onde o recurso está hospedado
            (arquivo, API ou serviço).
        """@pt-BR ;
  rdfs:comment """
            Endereço da web onde o recurso pode ser acessado.
        """@pt-BR ;
  rdfs:domain dcat:Distribution ;
  rdfs:range rdfs:Resource .

dc:format
  dc:identifier "dcterms:format" ;
  rdfs:label "Formato"@pt-BR, "Format"@en ;
  ns0:usageNote """
            Deve selecionar sob qual formato o recurso será disponibilizado.
        """@pt-BR ;
  rdfs:comment """
            Deve indicar o formato do recurso (por exemplo, CSV, JSON, PDF).
        """@pt-BR ;
  rdfs:domain dcat:Distribution ;
  rdfs:range dc:MediaTypeOrExtent .

